{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nutritionist/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nutritionist/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nutritionist",
  "projects": {
    "nutritionist": {
      "type": "application",
      "root": "apps/nutritionist",
      "entryFile": "main",
      "sourceRoot": "apps/nutritionist/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nutritionist/tsconfig.app.json"
      }
    },
    "app-config": {
      "type": "library",
      "root": "libs/app-config",
      "entryFile": "index",
      "sourceRoot": "libs/app-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/app-config/tsconfig.lib.json"
      }
    },
    "s3storage": {
      "type": "library",
      "root": "libs/s3storage",
      "entryFile": "index",
      "sourceRoot": "libs/s3storage/src",
      "compilerOptions": {
        "tsConfigPath": "libs/s3storage/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "util": {
      "type": "library",
      "root": "libs/util",
      "entryFile": "index",
      "sourceRoot": "libs/util/src",
      "compilerOptions": {
        "tsConfigPath": "libs/util/tsconfig.lib.json"
      }
    },
    "otp": {
      "type": "library",
      "root": "libs/otp",
      "entryFile": "index",
      "sourceRoot": "libs/otp/src",
      "compilerOptions": {
        "tsConfigPath": "libs/otp/tsconfig.lib.json"
      }
    },
    "mailer": {
      "type": "library",
      "root": "libs/mailer",
      "entryFile": "index",
      "sourceRoot": "libs/mailer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mailer/tsconfig.lib.json"
      }
    },
    "signature": {
      "type": "library",
      "root": "libs/signature",
      "entryFile": "index",
      "sourceRoot": "libs/signature/src",
      "compilerOptions": {
        "tsConfigPath": "libs/signature/tsconfig.lib.json"
      }
    },
    "app-jwt": {
      "type": "library",
      "root": "libs/app-jwt",
      "entryFile": "index",
      "sourceRoot": "libs/app-jwt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/app-jwt/tsconfig.lib.json"
      }
    }
  }
}